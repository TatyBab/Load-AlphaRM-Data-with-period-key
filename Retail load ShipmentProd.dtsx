      DTS:Expression="&quot;&#xA;SET NOCOUNT ON&#xA;&#xA;BEGIN TRY&#xA;&#xA;&#x9;BEGIN TRANSACTION&#xA;&#xA;&#xA;&#x9;&#x9;truncate table &quot;+ @[User::Table_Destination_Del] +&quot;&#xA;&#xA;&#x9;&#x9;insert into &quot;+ @[User::Table_Destination_Del] +&quot; with(tablock)&#xA;&#x9;&#x9;select * from &quot;+ @[User::Table_Destination_full] +&quot;&#xA;&#x9;&#x9;where [PERIOD_KEY] between &quot;+  @[User::period_key_from] +&quot; and &quot;+ @[User::period_key_to]  +&quot;&#xA;&#xA;&#x9;&#x9;delete &quot;+ @[User::Table_Destination_full] +&quot;&#xA;&#x9;&#x9;where [PERIOD_KEY] between &quot; + @[User::period_key_from] + &quot; and &quot;+ @[User::period_key_to]  +&quot;&#xA;&#xA;&#x9;&#x9;insert into &quot;+ @[User::Table_Destination_full] +&quot; with(tablock)&#xA;&#x9;&#x9;select * from &quot;+ @[User::Table_Destination_PartitionBuffer] +&quot; &#xA;&#x9;&#x9;where [PERIOD_KEY] between &quot;+ @[User::period_key_from] +&quot; and &quot;+ @[User::period_key_to]  +&quot;&#xA;&#xA;&#xA;&#x9;&#x9;truncate table &quot;+ @[User::Table_Destination_PartitionBuffer] +&quot;&#xA;&#xA;&#xA;&#x9;COMMIT TRANSACTION&#xA;&#xA;END TRY&#xA;BEGIN CATCH&#xA;&#xA;&#x9;IF @@TRANCOUNT &gt; 0&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;IF XACT_STATE() &lt;&gt; 0&#xA;&#x9;&#x9;&#x9;ROLLBACK TRANSACTION&#xA;&#x9;END;&#xA;&#xA;&#x9;THROW;&#xA;&#xA;END CATCH&#xA;&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SQL_Destination_full">
      <DTS:VariableValue
        DTS:DataType="8">
SET NOCOUNT ON

BEGIN TRY

	BEGIN TRANSACTION


		truncate table [stg].[ShipmentProd_Del]

		insert into [stg].[ShipmentProd_Del] with(tablock)
		select * from [stg].[ShipmentProd]
		where [PERIOD_KEY] between 202301 and 202306

		delete [stg].[ShipmentProd]
		where [PERIOD_KEY] between 202301 and 202306

		insert into [stg].[ShipmentProd] with(tablock)
		select * from [stg].[ShipmentProd_PartitionBuffer] 
		where [PERIOD_KEY] between 202301 and 202306


		truncate table [stg].[ShipmentProd_PartitionBuffer]


	COMMIT TRANSACTION

END TRY
BEGIN CATCH

	IF @@TRANCOUNT &gt; 0
	BEGIN
		IF XACT_STATE() &lt;&gt; 0
			ROLLBACK TRANSACTION
	END;

	THROW;

END CATCH
</DTS:VariableValue>
